                    $('#BOMCost1').val(data.BOMCost1);
                    $('#BOMCost2').val(data.BOMCost2);
                    $('#BOMCost3').val(data.BOMCost3);
                    $('#BOMCost4').val(data.BOMCost4);
                    $('#BOMCost5').val(data.BOMCost5);
                    $('#BOMPrice1').val(data.BOMPrice1);
